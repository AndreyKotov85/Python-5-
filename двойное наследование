class Animal:
  def __init__(self,name):
    self.name = name

class Horse(Animal):
  def run(self,distance):
    print(self.name, "run",distance, "metres")

class Bird(Animal):
  def fly(self, distance):
    print(self.name, "fly", distance, "meters")

class Unicorn(Horse, Bird):
  print

unicorn = Unicorn("Little")
unicorn.run(1000)
unicorn.fly(100)
